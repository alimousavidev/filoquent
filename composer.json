{
    "name": "alimousavi/filoquent",
    "description": "A powerful Laravel package for filtering Eloquent models using HTTP query parameters.",
    "type": "library",
    "homepage": "https://github.com/alimousavidev/filoquent",
    "license": "MIT",
    "version": "1.2.0",
    "authors": [
        {
            "name": "Ali Mousavi",
            "email": "hello@alimousavi.dev"
        }
    ],
    "require": {
        "php": ">=8.0",
        "illuminate/support": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "AliMousavi\\Filoquent\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AliMousavi\\Filoquent\\FiloquentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
